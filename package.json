{
  "name": "propertinext-api-v1.2",
  "version": "0.1.2",
  "description": "PropertiNEXT Base API for Main Purpose (v1.2).",
  "author": "dennis@propertinext.com",
  "scripts": {
    "start": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-node\"",
    "build": "npm run build-sass && npm run build-ts && npm run tslint && npm run copy-static-assets",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "test": "jest --forceExit --coverage --verbose",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build-sass": "node-sass src/public/css/main.scss dist/public/css/main.css",
    "watch-sass": "node-sass -w src/public/css/main.scss dist/public/css/main.css",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "fix-tslint": "tslint -c --fix tslint.json -p tsconfig.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon dist/server.js",
    "dev": "npm run build && npm run watch-debug",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "@mapbox/leaflet-omnivore": "^0.3.4",
    "async": "^2.6.0",
    "aws-sdk": "^2.713.0",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "date-fns": "^1.29.0",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "geojson": "^0.5.0",
    "http-errors": "^1.6.3",
    "imagemin": "^7.0.1",
    "imagemin-mozjpeg": "^9.0.0",
    "jsonwebtoken": "^8.2.1",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.4",
    "moment": "^2.27.0",
    "multer": "^1.4.2",
    "numeral": "^2.0.6",
    "numeral-es6": "^1.0.0",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "pug": "^2.0.0-rc.4",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^5.21.11",
    "sequelize-typescript": "^1.0.0-alpha.2",
    "validate.js": "^0.13.1",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.45",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/body-parser": "^1.16.8",
    "@types/compression": "^0.0.35",
    "@types/cors": "^2.8.3",
    "@types/dotenv": "^4.0.2",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.11.1",
    "@types/express-session": "^1.15.8",
    "@types/http-errors": "^1.6.1",
    "@types/jest": "^23.3.1",
    "@types/jquery": "^3.2.17",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/lodash": "^4.14.91",
    "@types/lusca": "^1.5.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.4.6",
    "@types/shelljs": "^0.7.8",
    "@types/supertest": "^2.0.4",
    "@types/winston": "^2.3.7",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "jest": "^23.4.2",
    "node-sass": "^4.7.2",
    "nodemon": "^1.13.0",
    "shelljs": "^0.8.1",
    "supertest": "^3.0.0",
    "ts-jest": "^23.0.1",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "typescript": "^3.9.6"
  }
}
